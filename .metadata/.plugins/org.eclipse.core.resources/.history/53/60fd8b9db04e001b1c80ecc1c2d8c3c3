for palabra in lista[:]:
    if palabra == 'stop':
        lista.insert(0, "ice ice babyy")
#print(lista)


for numero in range(1,20,6):
    print("El numerito ",numero," Y su cuadrado",numero**2)
    
for n in range(2, 10):
 for x in range(2, n):
  if n % x == 0:
   print(n, 'equals', x, '*', n//x)
   break
else:
# loop fell through without finding a factor
print(n, 'is a prime number')
